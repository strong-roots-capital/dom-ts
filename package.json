{
  "name": "dom-ts",
  "version": "0.6.0",
  "description": "fp-ts binding for the DOM",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "browser": "dist/index.browser.js",
  "typings": "dist/index.d.ts",
  "author": "Wayne Van Son",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/waynevanson/dom-ts"
  },
  "scripts": {
    "build": "parcel build src/index.ts",
    "prepublish": "yarn build && yarn test",
    "prepush": "yarn prepublish",
    "semantic-release": "semantic-release",
    "test": "jest --passWithNoTests",
    "watch": "jest --watchAll"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.13",
    "fp-ts": "^2.5.4",
    "fp-ts-contrib": "^0.1.19",
    "global-jsdom": "^7.0.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jsdom": "^16.2.2",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.1",
    "semantic-release-cli": "^5.4.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.0"
  },
  "peerDependencies": {
    "fp-ts": "^2.5.4",
    "fp-ts-contrib": "^0.1.19",
    "typescript": "^4.0.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-namespace-from"
    ]
  }
